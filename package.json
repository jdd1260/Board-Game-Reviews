{
  "name": "cs411teamjoel",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "cd client && yarn build && cd ..",
    "start:server": "dotenv node app.js",
    "start": "yarn build && yarn start:server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-db": "dotenv sequelize db:create",
    "migrate-db": "dotenv sequelize db:migrate",
    "install-db": "sh server/db/setup-db.sh && npm run create-db ",
    "new-migration": "sequelize migration:generate --name",
    "undo-migration": "dotenv sequelize db:migrate:undo",
    "run-seeds": "dotenv sequelize db:seed:all",
    "update": "npm install && cd client && npm install && cd .. && npm run build && npm run migrate-db"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sequelize-cli": "^5.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
