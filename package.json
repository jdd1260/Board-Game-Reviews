    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "qs": "^6.9.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@fast-csv/format": "^4.1.2",
    "@fast-csv/parse": "^4.1.2",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/react": "^5.3.17",
    "husky": "^4.2.3",
    "json5": "^2.1.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "sequelize-cli": "^5.5.1"
